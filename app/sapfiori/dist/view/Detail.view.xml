<mvc:View controllerName="sap.fiori.sapfiori.controller.Detail" 
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" 
    xmlns="sap.m">
    <Page id="detail" title="{i18n>DetailTitle}" showNavButton="true" navButtonPress="handleNavButtonPress" >
        <VBox id="_IDGenVBox1">
            <Text id="_IDGenText1" text="{ProductID}" />
            <Text id="_IDGenText2" text="{ProductName}" />
            <Text id="_IDGenText3" text="{UnitsInStock}" />
        </VBox>
    </Page>
</mvc:View>
